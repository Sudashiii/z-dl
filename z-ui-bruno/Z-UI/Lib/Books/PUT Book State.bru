meta {
  name: PUT Book State
  type: http
  seq: 7
}

put {
  url: {{base_url}}/api/library/{{book_id}}/state
  body: json
  auth: inherit
}

body:json {
  {
    "isRead": true,
    "excludeFromNewBooks": true
  }
}

vars:pre-request {
  base_url: http://localhost:5173
  book_id: 1
}

settings {
  encodeUrl: true
  timeout: 0
}
