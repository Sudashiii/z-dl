meta {
  name: PUT Upload Book File
  type: http
  seq: 4
}

put {
  url: {{base_url}}/api/library/{{file_name}}
  body: file
  auth: inherit
}

vars:pre-request {
  base_url: http://localhost:5173
  file_name: sample.epub
}

body:file {
  file: @file(/Users/sascha/Downloads/sample.epub) @contentType(application/epub+zip)
}

settings {
  encodeUrl: true
  timeout: 0
}
